{
    "description" : "Image collection format for Sentinel 2 Level 2A data in AWS",
    "references" : "https://roda.sentinel-hub.com/sentinel-s2-l2a/readme.html",
    "example" : "/vsis3/sentinel-s2-l2a/tiles/20/L/KP/2018/10/21/0/R20m/B03.jp2",
    "tags" : ["Sentinel", "Copernicus", "ESA", "TOA"],
    "pattern" : ".+.jp2",
    "images" : {
        "pattern" : ".*tiles((?:/[A-Za-z0-9]+){7})/.*"
    },
    "datetime" : {
        "pattern" : ".*tiles(?:/[A-Za-z0-9]+){3}((?:/[A-Za-z0-9]+){3}).*",
        "format" : "/%Y/%m/%d"
    },
